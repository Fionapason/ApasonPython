#:kivy 1.0.9

<GUI_GridLayout>:

    cols: 2

    Label:
        font_size: 30
        text: str(root.diluate_in_label)
    Label:
        font_size: 30
        text: str(root.diluate_in_display)
    Label:
        font_size: 30
        text: str(root.diluate_out_label)
    Label:
        font_size: 30
        text: str(root.diluate_out_display)
    Label:
        font_size: 30
        text: str(root.output_flow_label)
    Label:
        font_size: 30
        text: str(root.output_flow_display)


    Button:
        font_size: 30
        text: str(root.system_on_button_label)
        on_press: root.press_system_on()


    Button:
        font_size: 30
        text: str(root.submit_button_label_2)
        on_press: root.press_2()
